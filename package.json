{
  "name": "typescript-starter",
  "description": "React & TypeScript Starter with webpack, ts-jest and runtime environment variables. It comes with fp-ts ecosystem and pre-configured prettier, eslint, vscode, husky hooks and Dockerfile to build a deployable image of your app",
  "repository": "git@github.com:Stouffi/typescript-react-starter.git",
  "author": "Stouffi <steff.leb@orange.fr>",
  "license": "MIT",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "TS_NODE_PROJECT=\"webpack/tsconfig.json\" webpack --config webpack/webpack.prod.ts",
    "prestart": "react-env",
    "start": "TS_NODE_PROJECT=\"webpack/tsconfig.json\" webpack-dev-server --config webpack/webpack.dev.ts",
    "test": "jest --watch",
    "test-ci": "jest",
    "postinstall": "./scripts/ensure-deduplicate.sh"
  },
  "husky": {
    "hooks": {
      "pre-push": "./scripts/hooks/pre-push.sh"
    }
  },
  "devDependencies": {
    "@beam-australia/react-env": "3.0.8",
    "@testing-library/react": "11.2.5",
    "@types/html-webpack-plugin": "3.2.4",
    "@types/jest": "26.0.21",
    "@types/node": "^12.19.8",
    "@types/react": "17.0.3",
    "@types/react-dom": "17.0.3",
    "@types/webpack": "4.41.26",
    "@types/webpack-dev-server": "3.11.2",
    "@types/webpack-merge": "5.0.0",
    "@typescript-eslint/eslint-plugin": "4.19.0",
    "@typescript-eslint/parser": "4.19.0",
    "clean-webpack-plugin": "3.0.0",
    "css-loader": "5.1.4",
    "eslint": "7.22.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-prefer-arrow": "1.2.3",
    "eslint-plugin-prettier": "3.3.1",
    "eslint-plugin-react": "7.23.1",
    "fork-ts-checker-webpack-plugin": "6.2.0",
    "html-webpack-plugin": "5.3.1",
    "husky": "5.2.0",
    "jest": "26.6.3",
    "prettier": "2.2.1",
    "style-loader": "2.0.0",
    "ts-jest": "26.5.4",
    "ts-loader": "8.0.18",
    "ts-node": "9.1.1",
    "tsconfig-paths": "3.9.0",
    "typescript": "4.2.3",
    "webpack": "5.28.0",
    "webpack-cli": "4.1.0",
    "webpack-dev-server": "3.11.2",
    "webpack-merge": "5.7.3",
    "yarn-deduplicate": "3.1.0"
  },
  "dependencies": {
    "@matechs/console": "7.12.0",
    "@matechs/effect": "5.0.29",
    "antd": "4.14.1",
    "fast-check": "2.14.0",
    "fast-equals": "2.0.0",
    "firebase": "7.24.0",
    "fp-ts": "2.9.5",
    "fp-ts-contrib": "0.1.21",
    "io-ts": "2.2.16",
    "io-ts-types": "0.5.16",
    "monocle-ts": "2.3.9",
    "morphic-ts": "0.8.0-rc.2",
    "newtype-ts": "0.3.4",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-redux": "7.2.3",
    "redux": "4.0.5",
    "redux-observable": "1.2.0",
    "retry-ts": "0.1.2",
    "rxjs": "6.6.6"
  }
