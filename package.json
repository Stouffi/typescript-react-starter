{
  "name": "typescript-starter",
  "description": "React & TypeScript Starter with webpack, ts-jest and runtime environment variables. It comes with fp-ts ecosystem and pre-configured prettier, eslint, vscode, husky hooks and Dockerfile to build a deployable image of your app",
  "repository": "git@github.com:Stouffi/typescript-react-starter.git",
  "author": "Stouffi <steff.leb@orange.fr>",
  "license": "MIT",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "TS_NODE_PROJECT=\"webpack/tsconfig.json\" webpack --config webpack/webpack.prod.ts",
    "prestart": "react-env",
    "start": "TS_NODE_PROJECT=\"webpack/tsconfig.json\" webpack-dev-server --config webpack/webpack.dev.ts",
    "test": "jest --watch",
    "test-ci": "jest",
    "postinstall": "./scripts/ensure-deduplicate.sh"
  },
  "husky": {
    "hooks": {
      "pre-push": "./scripts/hooks/pre-push.sh"
    }
  },
  "devDependencies": {
    "@beam-australia/react-env": "3.1.1",
    "@testing-library/react": "12.0.0",
    "@types/html-webpack-plugin": "3.2.5",
    "@types/jest": "26.0.24",
    "@types/node": "^12.19.8",
    "@types/react": "17.0.13",
    "@types/react-dom": "17.0.8",
    "@types/webpack": "5.28.0",
    "@types/webpack-dev-server": "3.11.3",
    "@types/webpack-merge": "5.0.0",
    "@typescript-eslint/eslint-plugin": "4.28.2",
    "@typescript-eslint/parser": "4.28.2",
    "clean-webpack-plugin": "3.0.0",
    "css-loader": "5.2.6",
    "eslint": "7.30.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-prefer-arrow": "1.2.3",
    "eslint-plugin-prettier": "3.4.0",
    "eslint-plugin-react": "7.24.0",
    "fork-ts-checker-webpack-plugin": "6.2.12",
    "html-webpack-plugin": "5.3.2",
    "husky": "7.0.1",
    "jest": "26.6.3",
    "prettier": "2.3.2",
    "style-loader": "3.0.0",
    "ts-jest": "26.5.6",
    "ts-loader": "8.1.0",
    "ts-node": "10.0.0",
    "tsconfig-paths": "3.10.1",
    "typescript": "4.3.5",
    "webpack": "5.43.0",
    "webpack-cli": "4.1.0",
    "webpack-dev-server": "3.11.2",
    "webpack-merge": "5.8.0",
    "yarn-deduplicate": "3.1.0"
  },
  "dependencies": {
    "@matechs/console": "7.12.0",
    "@matechs/effect": "5.0.29",
    "antd": "4.16.6",
    "fast-check": "2.17.0",
    "fast-equals": "2.0.0",
    "firebase": "7.24.0",
    "fp-ts": "2.10.5",
    "fp-ts-contrib": "0.1.26",
    "io-ts": "2.2.16",
    "io-ts-types": "0.5.16",
    "monocle-ts": "2.3.10",
    "morphic-ts": "0.8.0-rc.2",
    "newtype-ts": "0.3.4",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-redux": "7.2.4",
    "redux": "4.1.0",
    "redux-observable": "2.0.0",
    "retry-ts": "0.1.3",
    "rxjs": "7.2.0"
  }
