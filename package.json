{
  "name": "typescript-starter",
  "description": "React & TypeScript Starter with webpack, ts-jest and runtime environment variables. It comes with fp-ts ecosystem and pre-configured prettier, eslint, vscode, husky hooks and Dockerfile to build a deployable image of your app",
  "repository": "git@github.com:Stouffi/typescript-react-starter.git",
  "author": "Stouffi <steff.leb@orange.fr>",
  "license": "MIT",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "TS_NODE_PROJECT=\"webpack/tsconfig.json\" webpack --config webpack/webpack.prod.ts",
    "prestart": "react-env",
    "start": "TS_NODE_PROJECT=\"webpack/tsconfig.json\" webpack-dev-server --config webpack/webpack.dev.ts",
    "test": "jest --watch",
    "test-ci": "jest",
    "postinstall": "./scripts/ensure-deduplicate.sh"
  },
  "husky": {
    "hooks": {
      "pre-push": "./scripts/hooks/pre-push.sh"
    }
  },
  "devDependencies": {
    "@beam-australia/react-env": "2.1.2",
    "@testing-library/react": "11.2.2",
    "@types/html-webpack-plugin": "3.2.4",
    "@types/jest": "26.0.15",
    "@types/node": "^12.19.7",
    "@types/react": "17.0.0",
    "@types/react-dom": "17.0.0",
    "@types/webpack": "4.41.25",
    "@types/webpack-dev-server": "3.11.1",
    "@types/webpack-merge": "4.1.5",
    "@typescript-eslint/eslint-plugin": "4.8.2",
    "@typescript-eslint/parser": "4.8.2",
    "clean-webpack-plugin": "3.0.0",
    "css-loader": "5.0.1",
    "eslint": "7.14.0",
    "eslint-config-prettier": "6.15.0",
    "eslint-plugin-prefer-arrow": "1.2.2",
    "eslint-plugin-prettier": "3.1.4",
    "eslint-plugin-react": "7.21.5",
    "fork-ts-checker-webpack-plugin": "6.0.4",
    "html-webpack-plugin": "4.5.0",
    "husky": "4.3.0",
    "jest": "26.6.3",
    "prettier": "2.2.0",
    "style-loader": "2.0.0",
    "ts-jest": "26.4.4",
    "ts-loader": "8.0.11",
    "ts-node": "9.0.0",
    "tsconfig-paths": "3.9.0",
    "typescript": "4.1.2",
    "webpack": "5.8.0",
    "webpack-cli": "4.1.0",
    "webpack-dev-server": "3.11.0",
    "webpack-merge": "5.4.0",
    "yarn-deduplicate": "3.1.0"
  },
  "dependencies": {
    "@matechs/console": "7.12.0",
    "@matechs/effect": "5.0.29",
    "antd": "4.8.6",
    "fast-check": "2.7.0",
    "fast-equals": "2.0.0",
    "firebase": "7.24.0",
    "fp-ts": "2.9.0",
    "fp-ts-contrib": "0.1.21",
    "io-ts": "2.2.13",
    "io-ts-types": "0.5.12",
    "monocle-ts": "2.3.3",
    "morphic-ts": "0.8.0-rc.2",
    "newtype-ts": "0.3.4",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-redux": "7.2.2",
    "redux": "4.0.5",
    "redux-observable": "1.2.0",
    "retry-ts": "0.1.2",
    "rxjs": "6.6.3"
  }
