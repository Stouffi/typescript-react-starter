{
  "name": "typescript-starter",
  "description": "React & TypeScript Starter with webpack, ts-jest and runtime environment variables. It comes with fp-ts ecosystem and pre-configured prettier, eslint, vscode, husky hooks and Dockerfile to build a deployable image of your app",
  "repository": "git@github.com:Stouffi/typescript-react-starter.git",
  "author": "Stouffi <steff.leb@orange.fr>",
  "license": "MIT",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "TS_NODE_PROJECT=\"webpack/tsconfig.json\" webpack --config webpack/webpack.prod.ts",
    "prestart": "react-env",
    "start": "TS_NODE_PROJECT=\"webpack/tsconfig.json\" webpack-dev-server --config webpack/webpack.dev.ts",
    "test": "jest --watch",
    "test-ci": "jest",
    "postinstall": "./scripts/ensure-deduplicate.sh"
  },
  "husky": {
    "hooks": {
      "pre-push": "./scripts/hooks/pre-push.sh"
    }
  },
  "devDependencies": {
    "@beam-australia/react-env": "2.1.2",
    "@testing-library/react": "10.0.4",
    "@types/html-webpack-plugin": "3.2.2",
    "@types/jest": "25.2.1",
    "@types/node": "^12.12.37",
    "@types/react": "16.9.34",
    "@types/react-dom": "16.9.7",
    "@types/webpack": "4.41.12",
    "@types/webpack-dev-server": "3.10.1",
    "@types/webpack-merge": "4.1.5",
    "@typescript-eslint/eslint-plugin": "2.30.0",
    "@typescript-eslint/parser": "2.30.0",
    "clean-webpack-plugin": "3.0.0",
    "css-loader": "3.5.3",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-prefer-arrow": "1.2.0",
    "eslint-plugin-prettier": "3.1.3",
    "eslint-plugin-react": "7.19.0",
    "fork-ts-checker-webpack-plugin": "4.1.3",
    "html-webpack-plugin": "4.3.0",
    "husky": "4.2.5",
    "jest": "25.5.3",
    "prettier": "2.0.5",
    "style-loader": "1.2.1",
    "ts-jest": "25.4.0",
    "ts-loader": "7.0.2",
    "ts-node": "8.10.0",
    "tsconfig-paths": "3.9.0",
    "typescript": "3.8.3",
    "webpack": "4.43.0",
    "webpack-cli": "3.3.11",
    "webpack-dev-server": "3.10.3",
    "webpack-merge": "4.2.2",
    "yarn-deduplicate": "2.0.0"
  },
  "dependencies": {
    "@matechs/console": "3.0.33",
    "@matechs/effect": "5.0.29",
    "antd": "4.2.0",
    "fast-check": "1.24.2",
    "fast-equals": "2.0.0",
    "firebase": "7.14.2",
    "fp-ts": "2.5.4",
    "fp-ts-contrib": "0.1.15",
    "io-ts": "2.2.1",
    "io-ts-types": "0.5.6",
    "monocle-ts": "2.1.0",
    "morphic-ts": "0.8.0-rc.2",
    "newtype-ts": "0.3.4",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-redux": "7.2.0",
    "redux": "4.0.5",
    "redux-observable": "1.2.0",
    "retry-ts": "0.1.2",
    "rxjs": "6.5.5"
  }
}
